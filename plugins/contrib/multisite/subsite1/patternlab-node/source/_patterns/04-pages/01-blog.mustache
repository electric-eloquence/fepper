<div class="page" id="page">
  {{> organisms-_main-header }}
  <div role="main">
    <h1 class="section-title">Our Outdoor Blog</h1>
    <div class="l-two-col">
      <div class="l-main">
        <section class="section latest-posts">
          
          <ul class="post-list">
            {{# latest-posts }}
              <li>{{> molecules-_main-media-block }}</li>
            {{/ latest-posts }}
          </ul>
          <a href="#" class="text-btn">View more posts</a>
        </section>
        {{> molecules-_main-pagination }}
      </div><!--end l-main-->

      <div class="l-sidebar">
        {{> organisms-_main-recent-tweets }}
      </div><!--end l-sidebar-->
    </div><!--end two-col-->
  </div><!--End role=main-->
  {{> organisms-_main-footer }}
</div>
