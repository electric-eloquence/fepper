{
  "paths" : {
    "source" : {
      "root": "./source/",
      "patterns" : "./source/_patterns/",
      "data" : "./source/_data/",
      "styleguide" : "./core/styleguide/",
      "patternlabFiles" : "./core/",
      "js" : "./source/_scripts",
      "images" : "./source/_assets",
      "fonts" : "./source/_styles/fonts",
      "css" : "./source/_styles/"
    },
    "public" : {
      "root" : "./public/",
      "patterns" : "./public/patterns/",
      "data" : "./public/data/",
      "styleguide" : "./public/styleguide/",
      "js" : "./public/_scripts",
      "images" : "./public/_assets",
      "fonts" : "./public/_styles/fonts",
      "css" : "./public/_styles"
    }
  },
  "styleGuideExcludes": [
    "templates",
    "pages"
  ],
  "defaultPattern": "all",
  "ignored-extensions" : ["scss", "DS_Store", "less"],
  "ignored-directories" : ["css-processors"],
  "debug": false,
  "ishControlsVisible": {
    "s": true,
    "m": true,
    "l": true,
    "full": true,
    "random": false,
    "disco": false,
    "hay": false,
    "mqs": false,
    "find": true,
    "views-all": true,
    "views-annotations": true,
    "views-code": true,
    "views-new": true,
    "tools-all": true,
    "tools-sync": false,
    "tools-shortcuts": true,
    "tools-docs": true
  },
  "ishMinimum": "240",
  "ishMaximum": "2600",
  "patternStateCascade": ["inprogress", "inreview", "complete"],
  "patternStates": {
  },
  "patternExportPatternPartials": [],
  "patternExportDirectory": "./pattern_exports/",
  "cacheBust": true
}
